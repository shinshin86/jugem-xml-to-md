{
  "name": "jugem-xml-to-md",
  "version": "0.0.3",
  "description": "This is a CLI tool to convert JUGEM-XML to markdown format.",
  "main": "dist/index.js",
  "bin": {
    "jugem-xml-to-md": "dist/index.js"
  },
  "scripts": {
    "start": "node dist/index.js",
    "build": "NODE_ENV=production webpack",
    "dev": "webpack && npm run start",
    "fmt": "eslint --fix '**/*ts' && prettier --write '**/*.{ts,js,json,md}'",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "markdown",
    "converter",
    "jugem-xml"
  ],
  "author": "shinshin86 <shinshin86npm@gmail.com> (https://github.com/shinshin86)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shinshin86/jugem-xml-to-md.git"
  },
  "bugs": {
    "url": "https://github.com/shinshin86/jugem-xml-to-md/issues"
  },
  "homepage": "https://github.com/shinshin86/jugem-xml-to-md#readme",
  "devDependencies": {
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "prettier": "^2.2.1",
    "shebang-loader": "0.0.1",
    "ts-loader": "^8.1.0",
    "typescript": "^4.2.4",
    "webpack": "^5.31.2",
    "webpack-cli": "^4.6.0"
  },
  "dependencies": {
    "fast-xml-parser": "^3.19.0",
    "glob": "^7.1.6"
  }
}
